{
    "version": 2,
    "builds": [
        {
            "src": "/public",
            "use": "@vercel/static"
        },
        {
            "src": "/api/index.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/(css|js|images|fonts|storage)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
